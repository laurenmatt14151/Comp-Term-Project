#TESTING: MULTI, DIVI, MODI

# TEST MULTI: 3 * 12 ==> 36
# TEST DIVI: 36 / 2 ==> 18
# TEST MODI: 18 % 10 ==> 8

            .ALLOC
THREE       .DATA =3
ACC         .DATA

            .BEGIN

MULT:       LOAD THREE
            MULTI 12
            STORE ACC
            WRITE ACC

DIVI:       DIVI 2
            STORE ACC
            WRITE ACC

MODI:       MODI 10
            STORE ACC
            WRITE ACC

            HALT
           .END
